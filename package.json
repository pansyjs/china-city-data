{
  "name": "@pansy/china-division",
  "version": "1.0.0",
  "description": "中国省市区数据",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "cityData"
  ],
  "scripts": {
    "build": "father-build && npm run json",
    "json": "ts-node -r tsconfig-paths/register --project script-tsconfig.json ./scripts/export-json.ts",
    "test": "walrus-test",
    "lint:commit": "walrus commitlint --env HUSKY_GIT_PARAMS",
    "prettier": "walrus prettier",
    "release": "walrus release"
  },
  "repository": "git@github.com:ts-react/china-city-data.git",
  "author": "wangxingkang <wangxingkang@sensoro.com>",
  "license": "MIT",
  "devDependencies": {
    "@walrus/cli": "^1.3.4",
    "@walrus/plugin-release": "^1.9.1",
    "@walrus/preset-lint": "^1.1.7",
    "@walrus/test": "^1.1.3",
    "cross-env": "7.0.3",
    "father-build": "1.19.1",
    "husky": "^4.3.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
